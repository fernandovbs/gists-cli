{
  "name": "gists-cli",
  "description": "Simple gists cli that allows you to find your gists quickly",
  "version": "0.0.1",
  "author": "Fernando Souza @fernandovbs",
  "bin": {
    "gists": "./bin/run"
  },
  "bugs": "https://github.com/fernandovbs/gists-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "cli-ux": "^5.4.9",
    "gists": "^2.0.0",
    "inquirer": "^7.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "inquirer-test": "^2.0.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/fernandovbs/gists-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "gists",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "fernandovbs/gists-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
